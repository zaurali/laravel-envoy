{
    "name": "sikic/laravel-envoy",
    "description": "Run envoy tasks from your controller",
    "type": "library",
    "require": {
        "symfony/process": "v5.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Luka",
            "email": "luka@sikic.eu"
        }
    ],
    "autoload": {
        "psr-4": {
